{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "rawblob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "rawdrop/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "optimizedblob",
      "type": "blob",
      "path": "processed/{name}",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
